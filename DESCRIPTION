Package: OpeNoise
Type: Package
Title: Environmental Noise Pollution Data Analysis
Version: 0.2-18
Authors@R: c(
 person("Pasquale", "Scordino", ,"scordino.pasquale@gmail.com" ,role = c("aut", "cre")), 
 person("Simone", "Sperotto", ,"s.sperotto@arpa.piemonte.it", role = "ctb"), 
